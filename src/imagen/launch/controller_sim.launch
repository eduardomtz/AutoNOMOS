<launch>
    <node name="imagenDisplay" pkg="imagen" type="imagenDisplay" output="screen" /> 
    
    <node name="lane_controller_sim_node" pkg="imagen" type="lane_controller_sim_node" output="screen" > 

    <param name="Kp"          type="double" value="2.0"/>
    <param name="Ki"          type="double" value="0.0"/>
    <param name="Kd"          type="double" value="0.0"/>
    <param name="dt"          type="double" value="1.0"/>
    <param name="pE"          type="double" value="320.0"/>
    <param name="min"          type="double" value="-0.5"/>
    <param name="max"          type="double" value="0.5"/>
    <param name="velocity"          type="double" value="-0.05"/>
    <param name="altura"          type="double" value="70.0"/>

    <param name="topico_steering"          type="string" value="/autonomos/steer/steer_position_controller/command"/>
    <param name="topico_velocidad"          type="string" value="/cmd_vel"/>

    </node> 
</launch>
